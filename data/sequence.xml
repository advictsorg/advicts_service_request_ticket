<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="service_request_sequence" model="ir.sequence">
            <field name="name">Service Request Sequence</field>
            <field name="code">service.request</field> <!-- This code must match -->
            <field name="active">TRUE</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="service_change_request_sequence" model="ir.sequence">
            <field name="name">Change Service Request Sequence</field>
            <field name="code">service.change.request</field>
            <field name="active">TRUE</field>
            <field name="prefix">CSR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_service_request" model="ir.sequence">
            <field name="name">Service Request Type Sequence</field>
            <field name="code">service.request</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>